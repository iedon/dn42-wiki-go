{
  "live": true,
  "editable": true,
  "listen": ":8080",
  "git": {
    "binPath": "git",
    "remote": "https://git.dn42.dev/wiki/wiki",
    "localDirectory": "./wiki",
    "pullIntervalSec": 3600,
    "author": "Anonymous <anonymous@localhost>",
    "commitMessagePrefix": "[wiki] ",
    "commitMessageAppendRemoteAddr": " (via %s)",
    "commandTimeoutSec": 120
  },
  "webhook": {
    "enabled": true,
    "secret": "",
    "polling": {
      "enabled": false,
      "endpoint": "https://git.dn42/dn42notify/poll",
      "callbackUrl": "https://dn42.jp/api/webhook/pull",
      "pollingIntervalSec": 3600,
      "skipRemoteCert": false
    }
  },
  "outputDir": "./dist",
  "templateDir": "./template",
  "homeDoc": "Home.md",
  "siteName": "DN42 Wiki",
  "baseUrl": "",
  "ignoreHeader": true,
  "ignoreFooter": false,
  "serverFooter": "Built with DN42 Wiki Go. You are accessing a distributed wiki node hosted by [IEDON-MNT](https://iedon.net).",
  "enableTLS": false,
  "tlsCert": "./server.crt",
  "tlsKey": "./server.key",
  "logLevel": "info",
  "trustedProxies": ["127.0.0.1", "::1/128", "172.16.0.0/16", "172.17.0.0/16", "172.18.0.0/16", "192.168.0.0/16"],
  "trustedRemoteAddrLevel": 1,
  "privatePagesPrefix": [
    "/internal"
  ]
}
